                            <a class="nav-link" href="cloth_Add">
                                <div class="sb-nav-link-icon"><i
                                        class="fas fa-tachometer-alt"></i></div>
                                Cloth_ADD
                            </a>
                            <a class="nav-link" href="//view_cloth">
                                <div class="sb-nav-link-icon"><i
                                        class="fas fa-tachometer-alt"></i></div>
                                View Cloth
                            </a>

                        </nav>
                    </div>
                    <div id="layoutSidenav_content">
                        <main>
                            <div class="col-6  mx-auto my-5 p-5 shadow">
                                <h2 class="text-center">Add Cloth</h2>
                                <form action="/api/cloth/cloth_Add"
                                    method="post" enctype="multipart/form-data">
                                    <div class="mt-4">
                                        Category:
                                        <select name="Catagory" id
                                            class="form-select">
                                            <option value="male">Male</option>
                                            <option value="female">female</option>
                                            <option value="children">Children</option>
                                        </select>
                                    </div>
                                    <div class="mt-4">
                                        Cloth_category:<br>
                                        <select name="cloth_catagory" id
                                            class="form-select">
                                            <option value="Tops">Tops</option>
                                            <option value="jeans">Jeans</option>
                                            <option value="shirt">Shirt</option>
                                            <option value="code-set">Code-Set</option>
                                            <option value="nightd-dress">Night-Dress</option>
                                        </select>
                                    </div>
                                    <div class="mt-4">
                                        Cloth_image: <input type="file"
                                            name="image" id
                                            class="form-control">
                                    </div>
                                    <div class="mt-4">
                                        cloth_title: <input name="cloth_title"
                                            id class="form-control"
                                            placeholder="Enter title">
                                    </div>
                                    <div class="mt-4">
                                        cloth_description: <textarea
                                            name="cloth_description" id
                                            class="form-control"
                                            placeholder="Enter Description"></textarea>
                                    </div>

                                    <div class="mt-4">
                                        cloth_quantity:
                                        <select name="cloth_quantity" id
                                            class="form-select">
                                            <option value="1">1</option>
                                            <option value="2">2</option>
                                            <option value="3">3</option>
                                            <option value="4">4</option>
                                            <option value="5">5</option>
                                        </select>
                                    </div>
                                    <div class="mt-4">
                                        cloth_price:
                                        <input type="number" name="cloth_price"
                                            id placeholder="Enter price"
                                            class="form-control">
                                    </div>

                                    <div class="mt-4">
                                        <button type="submit"
                                            class="btn btn-outline-danger">Add
                                            Cloath</button>
                                    </div>
                                    <!-- <div class="mt-4">
                                <a href="/signin" style="text-decoration: none;">All ready Registrer please login !</a>
                            </div> -->
                                </form>

                            </div>
                        </main>
                        <footer class="py-4 bg-light mt-auto">
